# Remove the line below if you want to inherit .editorconfig settings from higher directories
root = true

[*.cs]
indent_size = 4
indent_style = space
tab_width = 4
end_of_line = lf
insert_final_newline = true

[*]
indent_size = 2
indent_style = space
tab_width = 2
end_of_line = lf
insert_final_newline = true